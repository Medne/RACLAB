import numpy as np
import cv2
import os

def create_file():
	for img in os.listdir("neg"):
		line="neg"+'/'+img+'\n'
		with open('bg.txt','a') as f:
			f.write(line)

	for img in os.listdir("pos"):
		line="pos"+'/'+img+" 1 0 0 100 100\n"
		with open('info.lst','a') as f:
			f.write(line)
create_file()
